<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>Data Teachers</title>
  </head>
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.0/css/bootstrap.min.css" integrity="sha384-9gVQ4dYFwwWSjIDZnLEWnxCjeSWFphJiwGPXr1jddIhOegiu1FwO5qRGvFXOdJZ4" crossorigin="anonymous">
  <body>
  <h1>Data Teachers</h1>
  <table class="table">
  <thead>
    <tr>
      <th> First Name </th>
      <th> Last name </th>
      <th> Email </th>
      <th> Subject </th>
      <th> Options </th>
    </tr>
  </thead>
  <tbody>
    <% teachers.forEach(function(teacherData){ %>
      <tr>
        <td><%= teacherData.firstName %></td>
        <td><%= teacherData.lastName %></td>
        <td><%= teacherData.email %></td>
        <td><%= teacherData.Subject == null ? 'unassigned' : teacherData.Subject.subject %></td>
        <td> <a href="/teachers/edit/<%= teacherData.id%>">Edit </a> | <a href="/teachers/delete/<%= teacherData.id%>">Delete </a> </td>
      </tr>
    <% }) %>
  </tbody>
  </table>
  </body>

</html>
