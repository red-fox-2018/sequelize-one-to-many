<!DOCTYPE html>
<html>
<head>
    <title>Teachers</title>
</head>
<body>
    <h1>All Teachers Data</h1>
    <p>
        <a href="">Add Teachers Data</a>
    </p>
    <table border=1>
        <tr>
            <th>id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Subject</th>
            <th>Options</th>
        </tr>
        <% let teacher = teachers.sort(function(a,b){ return a.id-b.id }) %>
        <%   teacher.forEach(teacher=>{ %>

        <tr>
            <td><%=  teacher.id %></td>
            <td>  <%=  teacher.first_name  %>  </td>
            <td>  <%=  teacher.last_name  %>  </td>
            <td>  <%=  teacher.email  %>  </td>
            <td>unassigned</td>
            <td>
                <p><a href="">Edit</a> |
                <a href="">Delete</a></p>
            </td>
        </tr>
       <% }) %>
        
    </table>
</body>
</html>