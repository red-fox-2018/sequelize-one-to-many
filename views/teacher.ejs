<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Teacher's Page</title>
  </head>
  <body>
    <h1>Teachers Data</h1>
    <h3><a href="/teacher/add">add teacher</a></h3>
    <table style="width:80%" border="1">
      <tr>
        <th>No.</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Subject</th>
        <th>Options</th>
      </tr>
      <% teacherData.forEach((teachers) => { %>
      <tr>
        <td><%= teachers.id %></td>
        <td><%= teachers.first_name %></td>
        <td><%= teachers.last_name %></td>
        <td><%= teachers.email %></td>
          <td><%= unassigned(teachers.Subject) %></td>
        <td><a href=" /teacher/edit/<%= teachers.id %>">Edit</a> | <a href="/teacher/delete/<%= teachers.id %>" onclick="return confirm('Delete this teacher?')"> Delete </a></td>
      </tr>
      <% }) %>
    </table>
    <p><a href="/"> Back </a></p>
  </body>
</html>
